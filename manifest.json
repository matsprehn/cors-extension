{
	"name": "CORS Extension",
	"version": "1.0",
	"description": "Chrome extension for managing CORS",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"page_action": {
		"default_popup": "popup.html"
	},
	"permissions": ["webRequest", "webRequestBlocking", "*://localhost/*"]
}
