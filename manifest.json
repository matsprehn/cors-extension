{
	"name": "CORS Extension",
	"version": "1.0",
	"description": "Chrome extension for managing CORS",
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icons/cors-extension-disabled-64.png"
	},
	"permissions": ["storage", "webRequest", "webRequestBlocking", "<all_urls>"],
	"icons": { "64": "icons/cors-extension-enabled-64.png" }
}
